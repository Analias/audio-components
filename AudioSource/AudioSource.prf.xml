<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="audio-uri" mode="readwrite" type="string">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="loop-count" mode="readwrite" type="long">
    <description>The number of times to loop the audio.  Provide a negative number to loop indefinately.</description>
    <value>0</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="output-sample-rate" mode="readwrite" type="ulong">
    <description>Output sample rate</description>
    <value>8000</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="volume" mode="readwrite" type="double">
    <value>1</value>
    <range max="10" min="0"/>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="mute" mode="readwrite" type="boolean">
    <value>false</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="stream_id" mode="readwrite" type="string">
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
